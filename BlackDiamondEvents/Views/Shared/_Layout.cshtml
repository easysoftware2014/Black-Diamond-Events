        var keyPressCounter = 0;

        $("#phoneNumber").on("keypress", function(e) {

            if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                e.preventDefault();
            }

            var id = $(this).attr('id');
            var curchr = this.value.length;
            var curval = $(this).val();

            numberValidate(id, curchr, curval, e);

        });
        function numberValidate(id, curchr, curval, e) {

            var eventType = e.type;

            if (eventType == "paste") {
                var value = $("#" + id).val();
                console.log("value bo: " + value);
            }

            $("#" + id).keyup(function (e) {
                if (e.keyCode == 8) {
                    if (keyPressCounter == 4) {
                        curchr = curchr - 3;
                        keyPressCounter--;
                    } else {
                        curchr--;
                        keyPressCounter--;
                    }
                }

            });
            var newValue = "";
            var formatedInput;
            $("#" + id).focusout(function () {
                var id0 = "#" + id;
                $("#" + id).attr('minlength', "14");
                $("#" + id).attr('maxlength', "27");
                var val = $("#" + id).val();

                if (eventType === "paste") {
                    if (val) {

                        var validatePhoneNumber = /\(?([0-9]{3})\)?([ ]?)([0-9]{3})\2([0-9]{4})/;
                        if (!validatePhoneNumber.test(val)) {

                        } else {
                            newValue = val.toString().split("");
                            //formatedInput = formatInput(newValue);
                            $("#" + id).val(formatedInput);
                        }

                    }

                    eventType = "";
                    console.log("After split: " + newValue + " Formated: " + formatedInput);
                }
                if (curchr < 14) {
                    $("#" + id).attr('errorElement', 'show');

                }

            });
           

            if (curchr == 3) {

                $("#" + id).val("(" + curval + ")" + " ");
                curchr = curchr + 3;
            } else if (curchr == 9) {
                $("#" + id).val(curval + "-");
            }
            else if (curchr == 14) {
                $("#" + id).val(curval + "-");
            }
            else if (curchr == 19) {
                $("#" + id).val(curval + "-");
            }
            else if (curchr == 23) {
                $("#" + id).val(curval + "-");
            }
            else if (curchr == 27) {
                e.preventDefault();
            }
            keyPressCounter++;
        }
    });

</script>
